---
import AntLayout from "@layouts/AntLayout.astro";
import { appConfig } from "@core/config";
import Hero from "@components/hero.astro";
import Testimonials from "@components/testimonials.astro";
import innovations from "@assets/img/innovation.jpg";
import newdiag from "@assets/img/newdiag.jpg";
import reab from "@assets/img/reab.jpg";
import tele from "@assets/img/teleint.jpg";
import { Icon } from "astro-icon/components";
import medIcons from "@iconify-json/medical-icon/icons.json";

const prefix = medIcons.prefix;
type IconName = keyof typeof medIcons.icons;
const icon = (prefix: string, name: IconName): string => {
	return `${prefix}:${name}`;
};

